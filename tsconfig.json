{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "es2022",
    "lib": ["es2022"],
    "module": "Node16",
    "moduleResolution": "Node16",
    "noImplicitAny": true,
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "erasableSyntaxOnly": true
  }
}
