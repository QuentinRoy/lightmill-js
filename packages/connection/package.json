    "cross-env": "5.2.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.12.0",
    "promise.defer": "1.0.1",
    "rollup": "0.61.1",
