{
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": { "node": "current" }
          }
        ]
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "targets": { "browsers": ">1%" },
            "modules": false,
            "loose": true
          }
        ]
      ],
      "plugins": [
        "transform-object-rest-spread",
        [
          "transform-runtime",
          {
            "helpers": false,
            "polyfill": false,
            "regenerator": true
          }
        ],
        "external-helpers"
      ]
    }
  }
}
