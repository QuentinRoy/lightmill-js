{
  "name": "@lightmill/runner",
  "version": "3.0.0-alpha.5",
  "description": "JS Runner for LightMill.",
  "main": "dist/lightmill-runner",
  "module": "dist/lightmill-runner.mjs",
  "files": [
    "dist",
    "esm/**/!(*.test|*.spec).?(m)js"
  ],
  "license": "MIT",
  "access": "public",
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/plugin-external-helpers": "7.18.6",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "array-range": "1.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-preset-env": "1.7.0",
    "cross-env": "7.0.3",
    "jest": "29.3.1",
    "promise.defer": "1.0.1",
    "rollup": "1.32.1",
    "rollup-plugin-async": "1.2.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "wait-then": "0.1.2"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "coverage": "pnpm run test --coverage",
    "build-esm": "node ../../build-tools/esm-reexport.cjs -s ./esm/index.mjs -o ./dist/lightmill-runner.mjs -nd",
    "build-umd": "cross-env NODE_ENV=production rollup --config rollup.config.js",
    "build": "pnpm build-esm; pnpm build-umd",
    "prepublish": "pnpm run build"
  },
  "keywords": [
    "experiment",
    "lightmill"
  ],
  "author": "Quentin Roy <quentin@quentinroy.fr>",
  "repository": "https://github.com/QuentinRoy/lightmill-js/tree/master/packages/lightmill-connection",
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "mjs"
    ],
    "transform": {
      "^.+.m?js$": "babel-jest"
    },
    "collectCoverageFrom": [
      "esm/**/*.?(m)js?(x)"
    ]
  }
}
