{
  "name": "@lightmill/log-client",
  "version": "3.0.0-beta.34",
  "description": "Client for LightMill's log server.",
  "license": "MIT",
  "type": "module",
  "author": "Quentin Roy <contact@quentinroy.fr>",
  "access": "public",
  "files": [
    "dist"
  ],
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    "import": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    }
  },
  "devDependencies": {
    "@lightmill/log-api": "workspace:*",
    "@types/throttle-debounce": "^5.0.2",
    "@vitest/ui": "catalog:",
    "cross-env": "^7.0.3",
    "msw": "^2.7.3",
    "onchange": "^7.1.0",
    "openapi-typescript": "^7.6.1",
    "type-fest": "^4.39.1",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "openapi-fetch": "^0.13.5"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test vitest",
    "generate-api-types": "openapi-typescript node_modules/@lightmill/log-api/openapi.yaml --empty-objects-unknown --output ./generated/api.d.ts",
    "watch:generate-api-types": "pnpm run generate-api-types && onchange 'node_modules/@lightmill/log-api/openapi.yaml' --no-exclude -- pnpm run generate-api-types",
    "build": "pnpm run generate-api-types && tsc -b tsconfig.build.json",
    "prepublish": " pnpm run build"
  },
  "keywords": [
    "logger",
    "client"
  ]
}
